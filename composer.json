{
  "name": "zfr/zfr-oauth2-server",
  "description": "PHP library to create an OAuth 2 server",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "oauth",
    "oauth 2",
    "server"
  ],
  "homepage": "http://www.github.com/zf-fr/zfr-oauth2-server",
  "authors": [
    {
      "name": "MichaÃ«l Gallego",
      "email": "mic.gallego@gmail.com",
      "homepage": "http://www.michaelgallego.fr"
    },
    {
      "name": "Bas Kamer",
      "email": "baskamer@gmail.com",
      "homepage": "https://baskamer.nl"
    }
  ],
  "require": {
    "php": "^7.0",
    "zendframework/zend-diactoros": "^1.1",
    "psr/container": "^1.0",
    "ramsey/uuid": "^3.1",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.1",
    "phpunit/phpunit": "^6.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ZfrOAuth2\\Server\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZfrOAuth2Test\\Server\\": "tests/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9-dev"
    }
  }
}
